@model IkProjesi_1.Models.Unvan
@{
    ViewBag.Title = "UnvanDuzenle";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Merhabalar, Unvan Düzenleme Sayfası</h2>

<form action="@Url.Action("UnvanDuzenle","Unvan","Admin")" method="post">
    @Html.HiddenFor(x => x.Id)
    @Html.HiddenFor(x => x.AktifMi)
    <div class="form-group col-6">
        @Html.Label("Unvan Adı")
        @Html.TextBoxFor(x => x.ReservedName, new { @class = "form-control" })
    </div> 
    <button type="submit" class="btn btn-primary">Gönder</button>
</form>

